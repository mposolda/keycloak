        "bearerOnly": {
          "applicable": true,
          "read-only": true,
          "default-value": false
        },
        "standardFlowEnabled": {
          "applicable": true,
          "read-only": true,
          "default-value": false
        },
        "implicitFlowEnabled": {
          "applicable": true,
          "read-only": true,
          "default-value": false
        },
        "directAccessGrantsEnabled": {
          "applicable": true,
          "read-only": true,
          "default-value": false
        },
        "serviceAccountsEnabled": {
          "applicable": true,
          "read-only": true,
          "default-value": true
        },
        "protocolMappers": {
